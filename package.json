{
  "name": "on-leave-control-glip-bot-skill",
  "version": "0.1.3",
  "description": "On leave control bot skill for [ringcentral-personal-chatbot-js]",
  "main": "dist/server/index.js",
  "scripts": {
    "ngrok": "ngrok http -region ap 6066",
    "ngrok-p": "ngrok http -region ap 4100",
    "start": "node -r @babel/register ./node_modules/.bin/rcpf build/test-bot.js",
    "c": "webpack-dev-server --open --progress --colors --config build/webpack.config.babel.js",
    "compile": "babel src/server --out-dir dist/server",
    "release": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --colors --config build/webpack.config.babel.js",
    "p": "node ./node_modules/.bin/rcpf build/test-bot.js",
    "view": "node build/view.js",
    "build": "npm run clean && npm run compile && npm run release && npm run view",
    "clean": "node build/clean.js",
    "prepublishOnly": "npm run build",
    "i": "npm i && npm i --no-save ringcentral-personal-chatbot",
    "local-dynamo": "node -r @babel/register build/dynamo-local.js"
  },
  "keywords": [
    "RingCentral",
    "Chatbot",
    "AI"
  ],
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "antd": "3.18.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-import": "1.11.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "dynamodb-local": "0.0.29",
    "file-loader": "3.0.1",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.6.0",
    "moment": "2.29.4",
    "ngrok": "3.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "react-subx": "0.4.9",
    "serverless": "1.42.2",
    "shelljs": "0.8.3",
    "standard": "12.0.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "subx": "0.7.3",
    "url-loader": "1.1.2",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1",
    "yarn-upgrade-all": "0.5.0"
  },
  "dependencies": {
    "shortid": "2.2.14"
  },
  "peerDependencies": {
    "ringcentral-personal-chatbot": ">=0.10.8"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rc-personal-bot-framework/on-leave-control-glip-bot-skill.git"
  },
  "bugs": {
    "url": "https://github.com/rc-personal-bot-framework/on-leave-control-glip-bot-skill/issues"
  },
